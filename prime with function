#include<stdio.h>
#include<conio.h>
void main()
{
    int n;
    void Prime(int);
    printf("Enter the Number which you want to check \n");
    scanf("%d",&n);
    Prime(n);
    getch();

}
void Prime(int k)
{
    int i,b=0;
      
for(i=2;i<=k;i++)    
{    
if(k%i==0 )    
{    
printf("Number is not prime");    
b++;    
}    
}    
if(b==0 && b!=0 && b!=1 )    
printf("Number is prime");
else
printf("Not prime");     

}
