#include<stdio.h>
#include<conio.h>
#include<string.h>
void main()
{
    char str1[50],str2[50];
    int i;
    printf("Enter the first string \n");
    gets(str1);
    printf("Enter the second string \n");
    gets(str2);
    for(i=0;str1[i]!='\0'||str2[i]!='\0';i++)
    {
        if(str1[i]!=str2[i])
        {
            printf("Strings are not equal \n");
            break;
        }
        else
        {
            printf("Strings are equal \n");
            break;
        }
    }
    getch();
}
